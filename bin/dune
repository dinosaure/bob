(executable
 (name cat)
 (public_name bob.cat)
 (modules cat)
 (libraries bob.fiber))

(executable
 (name bob)
 (public_name bob)
 (modules bob client server relay connect args)
 (libraries base64 ipaddr ipaddr.unix cmdliner fpath fmt.cli logs.cli fmt.tty logs.fmt bob.unix))

(rule
 (target bob.com)
 (mode promote)
 (action (copy %{bin:bob} %{target})))
